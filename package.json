{
  "name": "clemans-net-access",
  "version": "0.0.1",
  "bin": {
    "index": "bin/index.ts"
  },
  "scripts": {
    "lint": "npx eslint .",
    "build": "npm run lint && npm run compile && npm run synth",
    "compile": "npx tsc",
    "synth": "JSII_SILENCE_WARNING_UNTESTED_NODE_VERSION=1 CDK_DEBUG=true  npx cdk synth",
    "deploy": "JSII_SILENCE_WARNING_UNTESTED_NODE_VERSION=1 CDK_DEBUG=true  npx cdk deploy --all --ci true || exit 1",
    "destroy": "JSII_SILENCE_WARNING_UNTESTED_NODE_VERSION=1 CDK_DEBUG=true  npx cdk destroy --all --force || exit 1",
    "start": "npm run deploy",
    "test": "npx jest --coverage",
    "watch": "npx tsc -w"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.19",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "aws-cdk": "2.128.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "markdownlint": "^0.33.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.128.0",
    "constructs": "^10.3.0",
    "dotenv": "^16.4.4"
  }
}
