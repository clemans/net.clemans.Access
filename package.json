{
  "name": "clemans-net-access",
  "author": "Brooks C. Clemans <brooks@clemans.net>",
  "version": "4.1.0",
  "types": "dist/bin/index.d.ts",
  "main": "dist/bin/index.js",
  "bin": {
    "clemans-net-access": "dist/bin/index.js"
  },
  "scripts": {
    "lint": "npx eslint . --fix",
    "build": "npm run lint && npm run compile && npm run synth && npm run diff",
    "compile": "npx tsc",
    "transpile": "npm run compile",
    "synth": "npx cdk synth | tee ./dist/Stack.yaml",
    "deploy": "npx cdk deploy --all --ci true || exit 1",
    "destroy": "npx cdk destroy --all --force || exit 1",
    "diff": "npx cdk diff",
    "start": "npm run deploy",
    "test": "npx jest --coverage",
    "watch": "npx tsc -w"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@jest/types": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "25.0.3",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "aws-cdk": "2.1100.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "markdownlint": "^0.40.0",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.233.0",
    "constructs": "^10.4.4",
    "dotenv": "^17.2.3",
    "ses-smtp-credentials-cdk": "^3.4.0"
  }
}
